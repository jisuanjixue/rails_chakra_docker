  "scripts": {
    "build": "esbuild app/javascript/*.* --bundle --sourcemap --outdir=app/assets/builds",
    "build:css": "tailwindcss -i /app/app/assets/stylesheets/application.tailwind.css -o /app/app/assets/builds/tailwind.css -c /app/config/tailwind.config.js"
  },
  "lint-staged": {
    "app/javascript/**/*.{js,ts,jsx,tsx,json}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "{package.json,.eslintrc.js,.prettierrc.js}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "./**/*.rb": [
      "bundle exec rubocop -a"
    ],
    "{Gemfile,Rakefile,config.ru}": [
      "bundle exec rubocop -a"
    ]
  }
